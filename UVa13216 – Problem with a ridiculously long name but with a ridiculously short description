#include<iostream>
#include<string>
using namespace std;

int main()
{
	int tc;
	cin>>tc;
	while(tc--)
	{
		string n;
		cin>>n;
		if(n=="0")
		cout<<1<<endl;
		else if(n=="1")
		cout<<66<<endl;
		else
		{
			if(n[n.length()-1]=='2'||n[n.length()-1]=='7')
			cout<<56<<endl;
			if(n[n.length()-1]=='3'||n[n.length()-1]=='8')
			cout<<96<<endl;
			if(n[n.length()-1]=='4'||n[n.length()-1]=='9')
			cout<<36<<endl;
			if(n[n.length()-1]=='5'||n[n.length()-1]=='0')
			cout<<76<<endl;
			if(n[n.length()-1]=='6'||n[n.length()-1]=='1')
			cout<<16<<endl;	
		}
	}	
}
