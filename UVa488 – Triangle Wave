#include<iostream>
using namespace std;

int main(){
	int n,a,f;
	cin>>n;
	while(n--)
	{
		cin>>a>>f;
		for(int k=0;k<f;k++)
		{
			for(int i=1;i<=a;i++,cout<<endl)
				for(int j=1;j<=i;j++)
					cout<<i;
			for(int i=a-1;i>=1;i--,cout<<endl)
				for(int j=i;j>=1;j--)
					cout<<i;
			if(n!=0||k!=f-1)
				cout<<endl;
		}
	}
}
